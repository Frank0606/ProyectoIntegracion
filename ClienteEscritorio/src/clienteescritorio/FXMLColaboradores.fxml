<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="680.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clienteescritorio.FXMLColaboradoresController">
    <children>
        <TableView fx:id="tablaColaboradores" layoutY="160.0" prefHeight="520.0" prefWidth="900.0">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" />
         </padding>
         <columns>
            <TableColumn fx:id="colNoPersonal" prefWidth="75.0" text="No. de personal" />
            <TableColumn fx:id="colNombre" prefWidth="75.0" text="Nombre" />
            <TableColumn fx:id="colTelefono" prefWidth="75.0" text="Teléfono" />
            <TableColumn fx:id="colCorreo" prefWidth="75.0" text="Correo electrónico" />
            <TableColumn fx:id="colCurp" prefWidth="75.0" text="CURP" />
            <TableColumn fx:id="colRol" prefWidth="75.0" text="Rol" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy></TableView>
      <Button fx:id="btnBuscar" layoutX="20.0" layoutY="80.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: #850309; -fx-border-radius: 60; -fx-background-radius: 60;" text="Buscar" textFill="WHITE">
         <font>
            <Font name="Candara Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnAgregar" layoutX="540.0" layoutY="80.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" style="-fx-background-color: #850309; -fx-border-radius: 60; -fx-background-radius: 60;" text="Agregar" textFill="WHITE">
         <font>
            <Font name="Candara Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnEditar" layoutX="650.0" layoutY="80.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: #850309; -fx-border-radius: 60; -fx-background-radius: 60;" text="Editar" textFill="WHITE">
         <font>
            <Font name="Candara Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnEliminar" layoutX="750.0" layoutY="80.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" style="-fx-background-color: #850309; -fx-border-radius: 60; -fx-background-radius: 60;" text="Eliminar" textFill="WHITE">
         <font>
            <Font name="Candara Bold" size="18.0" />
         </font>
      </Button>
      <TextField fx:id="barraBusqueda" layoutX="120.0" layoutY="80.0" prefHeight="40.0" prefWidth="400.0" promptText="Buscar colaborador por No. de personal" style="-fx-border-radius: 60; -fx-background-radius: 60; -fx-background-color: #E6E0E9; -fx-border-color: #850309;">
         <font>
            <Font name="Candara" size="18.0" />
         </font>
      </TextField>
    </children>
</AnchorPane>
