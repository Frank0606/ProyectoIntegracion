         <font>
            <Font name="Candara" size="18.0" />
         </font>
      </TextField>
      <Button fx:id="btnAsignar" layoutX="763.0" layoutY="80.0" mnemonicParsing="false" onAction="#btnAsignar" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: #850309; -fx-border-radius: 60; -fx-background-radius: 60;" text="Asignar" textFill="WHITE">
         <font>
            <Font name="Candara Bold" size="18.0" />
