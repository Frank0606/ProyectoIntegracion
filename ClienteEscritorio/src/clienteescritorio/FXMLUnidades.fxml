<AnchorPane prefHeight="680.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clienteescritorio.FXMLUnidadesController">
    <children>
        <TableView fx:id="tablaUnidades" layoutY="160.0" prefHeight="520.0" prefWidth="900.0">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" />
         </padding>
         <columns>
            <TableColumn fx:id="colVin" prefWidth="75.0" text="VIN" />
            <TableColumn fx:id="colNoIdentificacion" prefWidth="75.0" text="No. de identificacion" />
            <TableColumn fx:id="colMarca" prefWidth="75.0" text="Marca" />
            <TableColumn fx:id="colModelo" prefWidth="75.0" text="Modelo" />
            <TableColumn fx:id="colAnio" prefWidth="75.0" text="AÃ±o" />
            <TableColumn fx:id="colTipoUnidad" prefWidth="75.0" text="Tipo de unidad" />
            <TableColumn fx:id="colColaborador" prefWidth="75.0" text="Conductor" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy></TableView>
      <Button fx:id="btnBuscar" layoutX="20.0" layoutY="80.0" mnemonicParsing="false" onAction="#btnBuscar" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: #850309; -fx-border-radius: 60; -fx-background-radius: 60;" text="Buscar" textFill="WHITE">
         <font>
            <Font name="Candara Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnAgregar" layoutX="434.0" layoutY="80.0" mnemonicParsing="false" onAction="#btnAgregar" prefHeight="40.0" prefWidth="100.0" style="-fx-background-color: #850309; -fx-border-radius: 60; -fx-background-radius: 60;" text="Agregar" textFill="WHITE">
         <font>
            <Font name="Candara Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnEditar" layoutX="544.0" layoutY="80.0" mnemonicParsing="false" onAction="#btnEditar" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: #850309; -fx-border-radius: 60; -fx-background-radius: 60;" text="Editar" textFill="WHITE">
         <font>
            <Font name="Candara Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnEliminar" layoutX="644.0" layoutY="80.0" mnemonicParsing="false" onAction="#btnEliminar" prefHeight="40.0" prefWidth="100.0" style="-fx-background-color: #850309; -fx-border-radius: 60; -fx-background-radius: 60;" text="Eliminar" textFill="WHITE">
