<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="obtenerTipoUnidades">
    <select id = "tipoUnidades" resultType="pojo.TipoUnidad">
        Select idTipoUnidad, tipoUnidad from TipoUnidad;
    </select>
    
     <select id = "obtenerTipoUnidadNombre" resultType="pojo.TipoUnidad">
        SELECT tu.idTipoUnidad, tu.tipoUnidad
        FROM TipoUnidad tu
        WHERE tu.tipoUnidad = #{tipoUnidad};
    </select>
</mapper>

